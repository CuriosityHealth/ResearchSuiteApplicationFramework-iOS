{
  "layouts": [
    {
      "identifier": "survey_list",
      "type": "list",
      "onLoad": [],
      "items": [
        {
          "identifier": "baseline",
          "title": "Baseline Survey",
          "predicate": {
            "format": "$baselineDate == nil",
            "substitutions": {
              "baselineDate": {
                "type": "state",
                "identifier": "baselineCompletionDate"
              }
            }
          },
          "onTap": [
            {
              "type": "queueActivity",
              "activityID": "baseline"
            }
          ]
        },
        {
          "identifier": "daily",
          "title": "Daily Survey",
          "predicate": {
            "format": "$baselineDate != nil",
            "substitutions": {
              "baselineDate": {
                "type": "state",
                "identifier": "baselineCompletionDate"
              }
            }
          },
          "onTap": [
            {
              "type": "queueActivity",
              "activityID": "dailyStressAssessment"
            }
          ]
        },
        {
          "identifier": "home",
          "title": "Set Home Location",
          "onTap": [
            {
              "type": "queueActivity",
              "activityID": "home"
            }
          ]
        },
        {
          "identifier": "work",
          "title": "Set Work Location",
          "onTap": [
            {
              "type": "queueActivity",
              "activityID": "work"
            }
          ]
        },
        {
          "identifier": "yadlFull",
          "title": "YADL Full",
          "onTap": [
            {
              "type": "queueActivity",
              "activityID": "yadlFull"
            }
          ]
        },
        {
          "identifier": "yadlSpot",
          "title": "YADL Spot",
