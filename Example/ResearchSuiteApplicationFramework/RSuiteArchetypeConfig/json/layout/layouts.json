{
  "layouts": [
    {
      "identifier": "survey_list",
      "type": "list",
      "onLoad": [],
      "items": [
        {
          "identifier": "baseline",
          "title": "Baseline Survey",
          "predicate": {
            "format": "$baselineDate == nil",
            "substitutions": {
              "baselineDate": {
                "type": "valueInState",
                "identifier": "baselineCompletionDate"
              }
            }
          },
          "onTap": [
            {
              "type": "queueActivity",
              "activityID": "baseline"
            }
          ]
        },
        {
          "identifier": "daily",
          "title": "Daily Survey",
          "predicate": {
            "format": "$baselineDate != nil",
            "substitutions": {
              "baselineDate": {
                "type": "valueInState",
                "identifier": "baselineCompletionDate"
              }
            }
          },
          "onTap": [
            {
              "type": "queueActivity",
              "activityID": "dailyStressAssessment"
            }
          ]
        }
      ]
    },
    {
      "identifier":"settings_list",
      "type": "list",
      "onLoad": [],
      "items": [
        {
          "identifier": "logOut",
          "title": "Log Out",
          "onTap": [
            {
              "type": "queueActivity",
              "activityID": "baseline"
            }
          ]
        }
      ]
    },
    {
      "identifier": "main",
      "type": "tabBar",
      "onLoad": [],
      "tabs": [
        {
          "identifier": "survey_list",
          "title": "Surveys",
          "shortTitle": "Surveys",
        },
        {
          "identifier": "settings_list",
          "title": "Settings",
          "shortTitle": "Settings",
        }
      ]
    },
    {
      "identifier": "consent",
      "type": "consent",
      "onLoad": []
    },
    {
      "identifier": "onboarding",
      "type": "onboarding",
      "onLoad": []
    }
  ]
}
